{
  "build": {
    "yang": "yang/new",
    "plugin": [
      {
        "name": "yangtree_generator",
        "parameter": [
          {
            "name": "output",
            "value": "{yang}/tree"
          },
          {
            "name": "expand-grouping",
            "value": "true"
          }
        ]
      },
      {
        "name": "yang_comparator",
        "parameter": [
          {
            "name": "old-yang",
            "value": "yang/old"
          },
          {
            "name": "compare-type",
            "value": "tree"
          },
          {
            "name": "result",
            "value": "yang/result.xml"
          }
        ]
      }
    ]
  }
}